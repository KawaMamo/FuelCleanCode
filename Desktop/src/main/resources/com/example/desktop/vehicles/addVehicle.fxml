               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="18.0">
               <children>
                  <Label text="السائق" />
                  <Label text="رقم اللوحة" />
                  <Label text="مركز المرور" />
                  <Label text="الحجم" />
                  <Label text="المكتب" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label layoutX="133.0" layoutY="25.0" text="إضافة مركبة">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
